                "DeviceOSVersion": 23,
                "DeviceSubtype": 24,
                "DeviceType": 25,
                "DirectoryServiceName": 26,
                "EmbeddedAsset": 27,
                "InnerIdentity": 29,
                "LastAuthException": 30,
                "LicenseGID": 31,
                "LicenseOID": 32,
                "NAS-IP-Addr-Attr": 33,
                "NAS-Port-Attr": 34,
                "PostureProfileName": 35,
                "PostureResult": 36,
                "ProvisioningValues": 37,
                "ResultCode": 38,
                "ServiceCatName": 38,
                "SubauthenticatorName": 39,
                "TunnelProtocol": 40,
                "User-Name-Attr": 41
