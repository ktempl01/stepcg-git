{                
    "Ignition Server Access": {
        "display"   : "Ignition Server Access",
        "path"      : "/opt/www/syslog/ignition.log*",
        "max"       : 50,
        "refresh"   : 0,
        "count"     : 7,
        "notify"    : false,
        "thinit"    : [ "Date" , "Calling-Station-Id", "UserNameAttr" , "AuthName" , "AuthType" , "Authentication" , "Authorization" , "AccessPolicy" , "rule" , "provisioning" , "descr" ],
        "notify"    : false,
        "format"    : {
            "multiline": "Error",
            "regex"    : "|^(.*?) ([0-9]{1,2}) ([0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}) (.*?) (.*?) ([a-zA-Z0-9_\\\\-\\\\.]{1,256}).*catId=(.*?),.*msgId=(.*?),.*AuthServerName=(.*?),.*AuthenticationDecision=(.*?),.*AuthenticatorIpAddr=(.*?),.*AuthenticatorName=(.*?),.*AuthenticatorType=(.*?),.*AuthorizationDecision=(.*?),.*AuthorizationRuleIds=(.*?),.*Calling-Station-Id-Attr=(.*?),.*entialValidationPolicy=(.*?),.*Description=(.*?),.*DeviceMAC=(.*?),.*DeviceOSType=(.*?),.*DeviceOSVersion=(.*?),.*DeviceSubtype=(.*?),.*DeviceType=(.*?),.*DirectoryServiceName=(.*?),.*NAS-IP-Addr-Attr=(.*?),.*NAS-Port-Attr=(.*?),.*ProvisioningValues=(.*?),.*ServiceCatName=(.*?),.*User-Name-Attr=(.*?),.*UserId=(.*?)$|",
            "match"    : {
                            "Date": [ 1 , " " , 2 , " "  , 3 ],
                            "Facilty": 4,
                            "Severity": 5,
                            "IP": 6,
                            "catId": 7,
                            "msgId": 8,
                            "Cred-Policy": 17,
                            "Calling-Station-Id": 16,
                            "UserNameAttr": 29,
                            "UserId": 30,
                            "NAS-IP-Addr-Attr": 25,
                            "AuthName": 12,
                            "AuthType": 13,
                            "Authentication": 10,
                            "Authorization": 14,
                            "AccessPolicy": 28,
                            "rule": 15,
                            "provisioning": 27,
                            "descr": 18,
                            "DeviceType": 23,
                            "DeviceSubtype": 22
            },
            "types"    : {
                            "Date": "date:Y/m/d H:i:s/100",
                            "Facility": "txt",
                            "Severity": "txt",
                            "IP": "txt",
                            "catId": "txt",
                            "msgId": "txt",
                            "Cred-Policy": "txt",
                            "Calling-Station-Id": "txt",
                            "UserNameAttr": "txt",
                            "UserId": "txt",
                            "NAS-IP-Addr-Attr": "txt",
                            "AuthName": "txt",
                            "AuthType": "txt",
                            "Authentication": "badge:severity",
                            "Authorization": "badge:severity",
                            "AccessPolicy": "txt",
                            "rule": "txt",
                            "provisioning": "txt",
                            "descr": "badge:severity",
                            "DeviceType": "txt",
                            "DeviceSubtype": "txt"
            }
        }
    }
}
