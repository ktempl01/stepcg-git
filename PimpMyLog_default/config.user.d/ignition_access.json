{                
    "Ignition Server Access": {
        "display"   : "Ignition Server Access",
        "path"      : "/opt/www/syslog/ignition_access.log*",
        "max"       : 50,
        "refresh"   : 0,
        "count"     : 7,
        "notify"    : false,
