            "types"    : {
                "Date": "date:Y/m/d H:i:s/100",
                "Facility": "txt",
                "Severity": "txt",
                "IP": "txt",
                "catId": "numeral",
                "Id": "numeral",
                "UserId": "txt",
                "msgId": "numeral",
                "ADDomainAsset": "txt",
                "AssignedAsset": "txt",
                "AuthServerName": "txt",
