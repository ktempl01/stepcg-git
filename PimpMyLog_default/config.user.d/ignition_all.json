{
    "Ignition Server All": {
        "display"   : "Ignition Server All",
        "path"      : "/opt/www/syslog/ignition.log*",
        "count"     : 7,
        "refresh"   : 0,
        "notify"    : false,
        "format"    : {
            "regex" : "|^(.*?) ([0-9]{1,2}) ([0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}) (.*?) (.*?) ([a-zA-Z0-9_\\-\\.]{1,256}) (.*)|",
            "match": {
                "Date": [ 1 , " " , 2 , " "  , 3 ],
                "Facilty": 4,
                "Severity": 5,
                "IP": 6,
                "Message": 7
            },
            "types": {
                "Date": "date:Y/m/d H:i:s/100",
                "Facility": "txt",
                "Severity": "badge:severity",
                "IP": "ip",
                "Message": "txt"
            }
        }
    }
}
